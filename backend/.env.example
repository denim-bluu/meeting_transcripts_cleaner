# Streamlit Prototype - Environment Configuration

# OpenAI API Configuration
OPENAI_API_KEY=your_api_key_here

# Model Configuration (defaults to o3-mini)
CLEANING_MODEL=o3-mini
REVIEW_MODEL=o3-mini
CHUNK_MODEL=o3-mini
AGGREGATION_MODEL=o3-mini

# Environment (development, staging, production)
ENVIRONMENT=development

# Concurrency & rate limits
TRANSCRIPT_MAX_CONCURRENCY=20
TRANSCRIPT_RATE_LIMIT_PER_MINUTE=80
INTELLIGENCE_MAX_CONCURRENCY=8

# Logging
LOG_LEVEL=INFO
